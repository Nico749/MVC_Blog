<!-- Loop over each object in the projects array -->
{{#each notes as |note| }}
<div class="row mb-4 note">
  <div class="col-md-5">
    <h2>
      
      <a href="/note/{{note.id}}">{{note.notename}}</a>
    </h2>
 
    <p>Created by {{usertable.name}} on {{ format_date note.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{note.description}}
    </p>
    
  

</div>
</div>
{{/each}}
