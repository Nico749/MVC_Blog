<!-- Loop over each object in the projects array -->
{{#each notes as |note| }}
<div class="row mb-4 note">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{!-- {{{get_emoji}} --}}
      <a href="/note/{{note.id}}">{{note.name}}</a>
    </h2>
 
    <p>Created by {{note.user.name}} on {{format_date note.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{note.description}}
    </p>
  

</div>
</div>
{{/each}}
